<%= form_tag("/users", method: "post") do %>
  <%= label_tag(:email, "Email:") %> 
  <%= email_field(:user,:email) %> <br><br>

  <%= label_tag(:email, "username:") %> 
  <%= text_field(:user,:username) %> <br><br>

  <%= label_tag(:email, "password:") %> 
  <%= password_field(:user,:password) %> <br><br>

  <%= submit_tag("Create") %>
<% end %>